{
	"$schema": "https://frontmatter.codes/frontmatter.schema.json",
	"frontMatter.framework.id": "astro",
	"frontMatter.preview.host": "http://localhost:4321",
	"frontMatter.content.publicFolder": "public/media",
	"frontMatter.content.pageFolders": [
		{
			"title": "posts",
			"path": "[[workspace]]/src/content/posts"
		}
	],
	"frontMatter.taxonomy.contentTypes": [
		{
			"name": "default",
			"pageBundle": false,
			"previewPath": "posts",
			"filePrefix": null,
			"clearEmpty": true,
			"fields": [
				{
					"title": "Title",
					"name": "title",
					"type": "string",
					"single": true
				},
				{
					"title": "Summary",
					"name": "summary",
					"type": "string"
				},
				{
					"title": "CreatedAt",
					"name": "createdAt",
					"type": "datetime"
				},
				{
					"title": "Tags",
					"name": "tags",
					"type": "datetime"
				},
				{
					"title": "Content preview",
					"name": "imageUrl",
					"type": "image",
					"isPreviewImage": true
				}
			]
		}
	],
	"frontMatter.git.enabled": true
}
